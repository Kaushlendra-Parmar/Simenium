{
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "tailwindCSS.includeLanguages": {
    "html": "html",
    "javascript": "javascript",
    "css": "css"
  },
  "tailwindCSS.experimental.classRegex": [
    "class[Name]*\\s*[:=]\\s*['\"]([^'\"]*)['\"]",
    "className\\s*[:=]\\s*['\"]([^'\"]*)['\"]"
  ],
  "files.associations": {
    "*.css": "tailwindcss",
    ".browserslistrc": "plaintext"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "css.customData": [".vscode/css_custom_data.json"]
}
